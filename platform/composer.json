{
    "config": {
        "preferred-install": "dist",
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "installer-types": ["library", "component"],
        "installer-paths": {
            "../public/assets/composer-asset/{$vendor}/{$name}/": [
                "type:component",
                "fomantic/ui"
            ]
        }
    },
    "require": {
        "php": ">=7.2.5",
        "codeigniter4/framework": "4.0.4",
        "roave/security-advisories": "dev-master",
        "symfony/polyfill-php73": "^1.17",
        "symfony/polyfill-php74": "^1.17",
        "symfony/polyfill-php80": "^1.17",
        "symfony/polyfill-ctype": "^1.17",
        "symfony/polyfill-mbstring": "^1.17",
        "symfony/polyfill-iconv": "^1.17",
        "symfony/polyfill-intl-grapheme": "^1.17",
        "symfony/polyfill-intl-idn": "^1.17",
        "symfony/polyfill-intl-icu": "^1.17",
        "symfony/polyfill-intl-messageformatter": "^1.17",
        "symfony/polyfill-intl-normalizer": "^1.17",
        "symfony/polyfill-uuid": "^1.17",
        "symfony/string": "^5.1",
        "symfony/options-resolver": "^5.1",
        "symfony/process": "^5.1",
        "symfony/filesystem": "^5.1",
        "symfony/finder": "^5.1",
        "oomphinc/composer-installers-extender": "^1.1",
        "fg/multiplayer": "^1.2.1",
        "erusev/parsedown": "^1.7",
        "erusev/parsedown-extra": "^0.8.1",
        "pixel418/markdownify": "^2.3",
        "mustache/mustache": "^2.13",
        "netcarver/textile": "^3.7",
        "twig/twig": "^3.0",
        "twig/string-extra": "^3.0",
        "salesforce/handlebars-php": "^2.3",
        "scssphp/scssphp": "^1.1",
        "matthiasmullie/minify": "^1.3",
        "t1st3/php-json-minify": "^0.6.2",
        "fomantic/ui": "2.8.6",
        "components/jquery": "3.5.1",
        "components/highlightjs": "^9.16",
        "components/webfontloader": "^1.6",
        "scrivo/highlight.php": "v9.18.1.1",
        "desandro/imagesloaded": "4.1.4",
        "desandro/masonry": "4.2.2",
        "components/modernizr": "^3.5",
        "whichbrowser/parser": "^2.0"
    }
}
